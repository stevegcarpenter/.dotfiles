{
  "css.validate": false,
  "scss.validate": false,
  "less.validate": false,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "editor.rulers": [78, 120],
  "editor.renderWhitespace": "all",
  "vim.visualstar": true,
  "vim.hlsearch": true,
  "vim.enableNeovim": true,
  "vim.neovimPath": "/usr/local/bin/nvim",
  "vim.disableExtension": false,
  "vim.statusBarColorControl": true,
  "vim.statusBarColors.insert": "#039bed",
  "vim.statusBarColors.normal": "#77c577",
  "vim.statusBarColors.replace": "#ee5276",
  "vim.statusBarColors.visual": "#c352c3",
  "vim.statusBarColors.visualblock": "#c352c3",
  "vim.statusBarColors.visualline": "#c352c3",
  "explorer.confirmDelete": false,
  "materialTheme.fixIconsRunning": false,
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "j"],
      "after": ["<Esc>"]
    },
    {
      "before": ["<C-c>"],
      "after": ["<Esc>"]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "h"],
      "after": ["<C-w>", "h"]
    },
    {
      "before": ["<leader>", "j"],
      "after": ["<C-w>", "j"]
    },
    {
      "before": ["<leader>", "k"],
      "after": ["<C-w>", "k"]
    },
    {
      "before": ["<leader>", "l"],
      "after": ["<C-w>", "l"]
    },
    {
      "before": ["leader", "w"],
      "after": [],
      "commands": [
        {
          "command": "workbench.action.files.save",
          "args": []
        }
      ]
    }
  ],
  "vim.leader": "<space>",
  "workbench.colorCustomizations": {
    "statusBar.background": "#77c577",
    "statusBar.noFolderBackground": "#77c577",
    "statusBar.debuggingBackground": "#77c577"
  },
  "javascript.updateImportsOnFileMove.enabled": "never",
  "scrolloff.scrolloff": 2,
  "typescript.updateImportsOnFileMove.enabled": "never",
  "[json]": {},
  "go.inferGopath": true,
  "go.gotoSymbol.includeGoroot": true,
  "go.gotoSymbol.includeImports": true,
  "go.editorContextMenuCommands": {
    "toggleTestFile": false,
    "addTags": false,
    "removeTags": false,
    "testAtCursor": false,
    "testFile": false,
    "testPackage": false,
    "generateTestForFunction": false,
    "generateTestForFile": false,
    "generateTestForPackage": false,
    "testCoverage": false,
    "playground": false,
    "fillStruct": true
  },
  "go.autocompleteUnimportedPackages": true,
  "html.format.endWithNewline": true,
  "javascript.preferences.quoteStyle": "single",
  "terminal.integrated.rendererType": "dom",
  "todo-tree.highlights.enabled": true,
  "[javascript]": {
    "editor.tabSize": 2
  },
  "[typescript]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[css]": {
    "editor.tabSize": 2
  },
  "editor.detectIndentation": false,
  "editor.tabSize": 2,
  "todo-tree.tree.showScanModeButton": false,
  "debug.node.autoAttach": "on",
  "turboConsoleLog.delimiterInsideMessage": "-",
  "turboConsoleLog.quote": "'",
  "turboConsoleLog.includeFileNameAndLineNum": false,
  "workbench.sideBar.location": "right",
  "editor.cursorStyle": "line",
  "editor.lineNumbers": "on",
  "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
  "editor.wordWrap": "off",
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.colorTheme": "Noctis Minimus",
  "todo-tree.general.tags": [
    "BUG",
    "HACK",
    "FIXME",
    "TODO",
    "XXX",
    "[ ]",
    "[x]"
  ],
  "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
  "window.zoomLevel": 1,
  "editor.formatOnSave": true,
  "editor.formatOnSaveMode": "modificationsIfAvailable",
  "editor.defaultFormatter": "esbenp.prettier-vscode"
}
